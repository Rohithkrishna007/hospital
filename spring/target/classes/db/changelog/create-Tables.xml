<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="1" author="rohith">
        <createTable tableName="patients">
            <column name="patient_id" type="INT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="disease" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="in-time" type="Time">
                <constraints nullable="false"/>
            </column>
            <column name="out-time" type="Time">
                <constraints nullable="false"/>
            </column>
            <column name="assigned_doctor_id" type="INT">
                <constraints nullable="false"/>
            </column>
            <column name="join_date" type="DATE">
                <constraints nullable="false"/>
            </column>
            <column name="status" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="2" author="rohith">
        <createTable tableName="doctors">
            <column name="doctor_id" type="INT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="join_date" type="DATE">
                <constraints nullable="false"/>
            </column>
            <column name="disease" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="3" author="rohith">
        <createTable tableName="dean">
            <column name="dean_id" type="INT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="name" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>